{
  "name": "amq-sort-imports",
  "displayName": "amq-sort-imports",
  "description": "Sort ES6 imports automatically.",
  "version": "2.4.3",
  "galleryBanner": {
    "color": "#2980b9",
    "theme": "dark"
  },
  "bugs": {
    "url": "https://github.com/amatiasq/vsc-sort-imports/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amatiasq/vsc-sort-imports.git"
  },
  "icon": "assets/logo.png",
  "publisher": "amatiasq",
  "engines": {
    "vscode": "^1.6.0"
  },
  "homepage": "https://github.com/amatiasq/vsc-sort-imports/blob/master/README.md",
  "license": "ISC",
  "categories": [
    "Languages",
    "Other"
  ],
  "keywords": [
    "ES6",
    "ES2015",
    "Sort",
    "Sorting",
    "Import"
  ],
  "activationEvents": [
    "onCommand:extension.amqSortImports",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "amqSortImports.sort",
        "title": "Sort imports"
      },
      {
        "command": "amqSortImports.saveWithoutSorting",
        "title": "Save file without sorting imports"
      }
    ],
    "configuration": {
      "title": "Sort Imports configuration",
      "properties": {
        "amqSortImports.blankLinesAfter": {
          "type": "string",
          "default": true,
          "description": "Blank lines after import declarations."
        },
        "amqSortImports.sortType": {
          "type": "string",
          "default": "by-module-name",
          "description": "Sorting algorithm."
        },
        "amqSortImports.onSave": {
          "type": "boolean",
          "default": true,
          "description": "Automatically sort imports on save."
        },
        "amqSortImports.suppressWarnings": {
          "type": "boolean",
          "default": false,
          "description": "Suppress warnings when sorting fails."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^6.0.46",
    "import-sort-parser": "^2.0.14",
    "import-sort-style": "^2.3.0",
    "typescript": "^2.0.6",
    "vscode": "^1.0.3"
  },
  "dependencies": {
    "@types/core-js": "^0.9.41",
    "import-sort": "2.4.0",
    "import-sort-config": "2.4.0",
    "import-sort-parser-babylon": "2.3.0",
    "import-sort-parser-typescript": "2.2.1",
    "import-sort-style-eslint": "2.4.0",
    "import-sort-style-module": "2.4.0"
  }
}
